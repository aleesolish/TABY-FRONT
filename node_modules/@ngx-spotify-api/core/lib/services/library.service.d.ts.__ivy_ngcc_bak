import { HttpClient } from '@angular/common/http';
import { Observable } from 'rxjs';
import { Paging } from '../models';
import { Album } from '../models';
import { Track } from '../models';
import { ApiConfig } from '../models';
export declare class LibraryService {
    private config;
    private http;
    constructor(config: ApiConfig, http: HttpClient);
    areSavedAlbums(ids: string[]): Observable<boolean[]>;
    areSavedTracks(ids: string[]): Observable<boolean[]>;
    getSavedAlbums(limit?: number, offset?: number, market?: string): Observable<Paging<Album>>;
    getSavedTracks(limit?: number, offset?: number, market?: string): Observable<Paging<Track>>;
    saveAlbums(ids: string[]): Observable<boolean>;
    removeAlbums(ids: string[]): Observable<boolean>;
    saveTracks(ids: string[]): Observable<boolean>;
    removeTracks(ids: string[]): Observable<boolean>;
}
