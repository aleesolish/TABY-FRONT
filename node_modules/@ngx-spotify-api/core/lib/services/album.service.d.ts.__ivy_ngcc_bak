import { HttpClient } from '@angular/common/http';
import { Observable } from 'rxjs';
import { Album } from '../models';
import { Paging } from '../models';
import { Track } from '../models';
import { ApiConfig } from '../models';
export declare class AlbumService {
    private config;
    private http;
    constructor(config: ApiConfig, http: HttpClient);
    getAlbum(id: string, market?: string): Observable<Album>;
    getAlbumTracks(id: string, limit?: number, offset?: number, market?: string): Observable<Paging<Track>>;
    getAlbums(ids: string[], market?: string): Observable<Album[]>;
}
