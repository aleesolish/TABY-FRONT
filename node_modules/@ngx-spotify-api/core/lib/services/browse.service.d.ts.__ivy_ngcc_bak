import { HttpClient } from '@angular/common/http';
import { Observable } from 'rxjs';
import { Category } from '../models';
import { Paging } from '../models';
import { FeaturedPlaylists } from '../models';
import { AlbumSimplified } from '../models';
import { ApiConfig } from '../models';
import { PlaylistSimplified } from '../models';
export declare class BrowseService {
    private config;
    private http;
    constructor(config: ApiConfig, http: HttpClient);
    getCategory(id: string, country?: string, locale?: string): Observable<Category>;
    getCategoryPlaylists(id: string, country?: string, limit?: number, offset?: number): Observable<Paging<PlaylistSimplified>>;
    getCategories(country?: string, locale?: string, limit?: number, offset?: number): Observable<Paging<Category>>;
    getFeaturedPlaylists(country?: string, locale?: string, timestamp?: Date, limit?: number, offset?: number): Observable<FeaturedPlaylists>;
    getNewReleases(country?: string, limit?: number, offset?: number): Observable<Paging<AlbumSimplified>>;
}
